.write
    width: 100%

    .square
        width: 313px
        padding: 32px 30px 34px 40px

        h1
            width 640px
            font-size 27px
            line-height: 38px
            margin: 12px 0 18px 0

        h3
            font-size 17px
            line-height: 17px
            padding: 0 0 10px 0
            margin: 0 0 12px 0

        h6
            width 510px
            margin: 4px 120px 24px 30px
            font-size 20px
            line-height: 30px

        p
            width 270px
            font-size: 16px
            line-height: 24px
            margin 0

        p.full
            width 660px
            margin 0 0 18px 0

        p.large
            display block
            width 660px
            margin 0 0 18px 0

        img
            width: 200px
            height: 238px
            margin: 0 30px 34px 0

        #map
            width: 688px
            height: 280px
            margin: 20px 0 20px 0
            border $border

        .profile
            display block
            float left
            width 170px
            height 209px
            margin 0 30px 0 0
            background #fff url(../img/profile/1.jpg) no-repeat;
            border $border

    .square.line
        border-right 1px dotted #ccc

    .square.end
        border-bottom none

.testimonials
    width 100%

    .testimonial
        padding: 40px 0 20px 0
        margin 0

        p
            width 620px
            margin 0 0 22px 60px
            font-size: 17px
            line-height: 29px

        h6
            width 460px
            margin 0 0 0 60px
            font-size: 17px
            line-height: 17px

    .testimonial-image
        width: 100px
        height: 120px
        margin: 4px 28px 0 60px


