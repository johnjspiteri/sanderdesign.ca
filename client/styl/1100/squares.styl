.squares
	width 1022px
	margin 0
	border $white-border
	border-bottom none

.square
	width 195px
	padding 32px 30px 30px 30px

	h1
		width 820px
		font-size 25px
		line-height 35px
		margin 0 0 16px 0

	h3
		font-size 17px
		line-height 17px
		padding 0 0 10px 0
		margin 0 0 14px 0

	h6
		width 510px
		margin: 8px 400px 28px 40px
		font-size 20px
		line-height: 29px

	p
		width 184px
		font-size: 15px
		line-height: 26px
		margin 0

	p.full
		width 940px
		margin 0 0 14px 0
		font-size: 16px
		line-height 26px

	p.large
		display block
		width 940px
		margin 0 0 14px 0
		font-size: 16px
		line-height 26px

	img
		width: 200px
		height: 238px
		margin: 0 60px 34px 0

	#map
		width: 960px
		height: 280px
		margin: 30px 0 0 0

.square.last
	width 194px

.square.plug
	border-right 1px dotted #ccc