.write
    width: 100%

    .square
        width: 100%
        padding: 32px 0 32px 0

        h1
            width 400px
            font-size 25px
            line-height: 36px
            margin: 0 0 18px 100px

        h3
            font-size 17px
            line-height: 17px
            padding: 0 0 8px 0
            margin: 0 260px 10px 100px

        h6
            width 400px
            margin: 0 0 20px 100px
            font-size 20px
            line-height: 29px

        p
            width: 410px
            font-size: 16px
            line-height: 27px
            margin 0 0 0 100px

        p.full
            width 440px
            margin 0 0 16px 100px
            font-size: 17px
            line-height: 26px

        .service
            margin: 0 220px 32px 100px

        #map
            width: 458px
            height: 220px
            margin: 30px 0 0 100px

.testimonials
    width 100%

    .testimonial
        padding: 30px 0 20px 0
        margin 0

        p
            width 460px
            margin 0 0 22px 100px
            font-size: 16px
            line-height: 27px

        h6
            width 460px
            margin 0 0 0 100px
            font-size: 17px
            line-height: 17px

    .testimonial-image
        width: 100px
        height: 120px
        margin: 0 22px 0 100px


