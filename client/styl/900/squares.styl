.squares
	width: 920px
	margin 0 0 0 52px
	border $white-border
	border-bottom none

.square
	width: 379px
	padding: 32px 40px 42px 40px

	h1
		width 820px
		font-size 27px
		line-height: 37px
		margin: 0 0 18px 0

	h3
		font-size 22px
		line-height 22px
		padding 0 0 10px 0
		margin 0 0 15px 0

	h6
		width 620px
		margin: 4px 260px 24px 40px
		font-size 21px
		line-height: 32px

	p
		width 370px
		font-size: 19px
		line-height: 32px
		margin 0

	p.full
		width 820px
		margin 0 0 18px 0
		font-size: 17px
		line-height: 27px

	p.large
		display block
		width 820px
		margin 0 0 18px 0
		font-size: 17px
		line-height: 27px

	.service
		width: 200px
		height: 238px
		margin: 0 130px 34px 0

	#map
		width: 840px
		height: 340px
		margin: 30px 0 0 0

.square.line
	border-right 1px dotted #ccc